int main(void)
{
	
	led_init();
	button_init();
	bool b = false;
    /* Replace with your application code */
    while (1) 
    {
		_delay_ms(120);
		if(button_read(1) == 1){
			b = !b;
		}
		if(b==true){
			led_on(0);
			led_on(1);
			led_on(2);
			led_on(3);
			led_on(4);
			
			
		} else {
			led_off(0);
			led_off(1);
			led_off(2);
			led_off(3);
			led_off(4);
		}
		
		
		
    }
}

